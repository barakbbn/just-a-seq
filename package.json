{
  "name": "@barakbbn/just-a-seq",
  "version": "1.2.0",
  "description": "Lazy Typescript sequence/collection (Like LINQ) with api that resembles JavaScript Array.",
  "scripts": {
    "build": "npm run tsup",
    "test": "ts-mocha -p ./tsconfig-mocha.json test/index.ts",
    "deploy": "npm install && npm run build && npm test && npm publish",
    "coverage": "nyc --reporter=lcov npm test",
    "tsup": "npx -y -p tsup@6.7.0 -p esbuild-plugin-umd-wrapper@1.0.7 -p @rollup/plugin-typescript@11.1.0 tsup"
  },
  "author": "barakbbn",
  "license": "ISC",
  "engines": {
    "npm": ">=7.0.0"
  },
  "repository": "https://github.com/barakbbn/just-a-seq",
  "homepage": "https://github.com/barakbbn/just-a-seq/wiki/docs",
  "devDependencies": {
    "@types/chai": "4.3.3",
    "@types/mocha": "10.0.0",
    "@types/node": "14.18.36",
    "chai": "4.3.7",
    "mocha": "10.2.0",
    "nyc": "15.1.0",
    "ts-mocha": "10.0.0",
    "ts-node": "10.9.1",
    "typescript": "4.1.5"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ]
    }
  },
  "keywords": [
    "LINQ",
    "iterable",
    "iterator",
    "generator",
    "enumerable",
    "IEnumerable",
    "lazy",
    "deferred",
    "sequence",
    "collection",
    "Typescript",
    "Javascript",
    "ES2018"
  ]
}
