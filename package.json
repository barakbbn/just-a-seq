{
  "name": "@barakbbn/just-a-seq",
  "version": "0.2.4",
  "description": "Lazy Typescript sequence/collection (Like LINQ) with api that resembles JavaScript Array.",
  "scripts": {
    "build": "tsc",
    "test": "ts-mocha -p ./tsconfig-mocha.json test/index.ts",
    "pack": "pushd dist && npm pack || popd",
    "deploy": "npm install && npm run build && npm test && npm run pack",
    "coverage": "nyc --reporter=lcov npm test",
    "_test_old": "mocha -r ts-node/register test/**/*.ts",
    "_deleteDist": "rm -rf ./dist 2> .err || rd /S /Q dist 2> .err"
  },
  "keywords": [
    "LINQ",
    "iterable",
    "iterator",
    "generator",
    "enumerable",
    "IEnumerable",
    "lazy",
    "deferred",
    "sequence",
    "collection",
    "Typescript",
    "Javascript",
    "ES2018"
  ],
  "author": "barakbbn",
  "license": "ISC",
  "repository": "https://github.com/barakbbn/just-a-seq",
  "homepage": "https://github.com/barakbbn/just-a-seq/wiki/docs",
  "devDependencies": {
    "@types/chai": "4.3.0",
    "@types/mocha": "9.1.0",
    "@types/node": "14.14.26",
    "chai": "4.3.6",
    "mocha": "10.0.0",
    "nyc": "15.1.0",
    "ts-mocha": "10.0.0",
    "ts-node": "10.7.0",
    "typescript": "4.1.5"
  },
  "files": [
    "dist/*"
  ],
  "exports": {
    ".": {
      "default": "./dist/index.js"
    },
    "./optimized": {
      "default": "./dist/optimized.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ],
      "optimized": [
        "./dist/optimized.d.ts"
      ]
    }
  }
}
